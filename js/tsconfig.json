{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "es2022",
    "target": "es2022",
    "declaration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",    
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
  }
}